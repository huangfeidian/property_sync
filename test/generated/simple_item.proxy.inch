
#ifndef __meta_parse__

template <> 
class prop_record_proxy<spiritsaway::test::simple_item>
	
{
	spiritsaway::test::simple_item& m_simple_item_data;
	item_msg_queue m_queue;
	const property_flags m_flag;
public:

public:
	prop_record_proxy(spiritsaway::test::simple_item& data, msg_queue_base& msg_queue,
		const property_offset& offset, const property_flags& flag, std::uint32_t data_idx)
		: m_queue(msg_queue, offset, flag, data_idx)
		, m_simple_item_data(data)
	{

	}
	

	




	prop_record_proxy<decltype(m_simple_item_data.m_a)> a()
	{
		property_offset empty_offset;
		return prop_record_proxy<decltype(m_simple_item_data.m_a)>(m_simple_item_data.m_a, m_queue, empty_offset.merge(spiritsaway::test::simple_item::index_for_a), m_flag.merge(property_flags{ spiritsaway::test::simple_item::flag_for_a}));
	}
	prop_record_proxy<decltype(m_simple_item_data.m_b)> b()
	{
		property_offset empty_offset;
		return prop_record_proxy<decltype(m_simple_item_data.m_b)>(m_simple_item_data.m_b, m_queue, empty_offset.merge(spiritsaway::test::simple_item::index_for_b), m_flag.merge(property_flags{ spiritsaway::test::simple_item::flag_for_b}));
	}


};
#endif